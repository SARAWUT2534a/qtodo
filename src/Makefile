####### Install

install_qtodotranslations: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/" || $(MKDIR) "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/"
	-$(INSTALL_FILE) "qt_de.qm" "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/"
	-$(INSTALL_FILE) "qtodo_de.qm" "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/"


uninstall_qtodotranslations: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/qt_de.qm"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/qtodo_de.qm"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/local/share/qtodo/translations/"


install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/local/bin/" || $(MKDIR) "$(INSTALL_ROOT)/usr/local/bin/"
	-$(INSTALL_FILE) "../bin/$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/local/bin/"


install: install_qtodotranslations install_target  

uninstall: uninstall_qtodotranslations uninstall_target  

